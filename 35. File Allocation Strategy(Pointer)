#include <stdio.h>
#define MAX_BLOCKS 10
int main() {
    int indexBlock[MAX_BLOCKS] = {5, 7, 9, 3};
    int blockCount = 4;
    printf("File allocated with %d blocks.\nIndex Block -> ", blockCount);
    for (int i = 0; i < blockCount; i++) {
        printf("Block %d ", indexBlock[i]);
    }
    printf("\n");

    return 0;
}
